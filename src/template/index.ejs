                    let count = 1125;
  let customElement = $("<div>", {
      id: "countdown",
      css: {
          "font-size" : "50px"
      },
      text: count
  });

  $("#el").busyLoad("show", {
      background: "brown",
      custom: customElement
  });

  let interval = setInterval(function(){ 
      count--;
      customElement.text(count);
      if (count <= 0) {
          clearInterval(interval);
          $("#el").busyLoad("hide");
          return;
      }
  }, 1000);
